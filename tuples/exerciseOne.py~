#!/usr/bin/env python3
#This is a example of a tuple

NewTuple = ('car', 2001, 1000)

#Prints elements of the tuple

print(NewTuple[2], "Is the second element of the New tuple")
print("")

#Convert tuple to list

convertTuple = list(NewTuple)
print(convertTuple, "Now is a list.")
print("")
#Convert tuples to list

newList = ["Luna", 12, "317"]
print(newList, "Is the new list.")
print("")

#Convert list to tuples
convertlist = tuple(newList)
print(convertlist, "Now is a list.")
print("")

#Check if the tuple have a element
otherTuple = ("Monday", 28, 2021)
print(28 in otherTuple, "Check with the function in if the elemen exist.")
print("")
print(otherTuple.count("Monday"), "How many time is the same element.")
print("")
for letter in "ABCDEFGHIJKLMNOPQRSTUVWXYZ":
    if letter in "AEIOU":
        if letter in "AEIOU":
            print(letter, "is a vowel")
        else:
            print(letter, "is a consonant")
